{
  "name": "code-make",
  "displayName": "Code Make",
  "description": "Simplify C, C++, Go, and Java project creation and compilation with Make.",
  "version": "3.0.0",
  "publisher": "drizzy",
  "license": "MIT",
  "icon": "icons/logo.png",
  "engines": {
    "vscode": "^1.71.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/drizzy/code-make.git"
  },
  "homepage": "https://github.com/drizzy/code-make/blob/master/README.md",
  "bugs": {
    "url": "https://github.com/drizzy/code-make/issues"
  },
  "author": {
    "name": "Willow P.",
    "email": "hello@drizzy.dev",
    "url": "https://drizzy.dev"
  },
  "sponsor": {
    "url": "https://github.com/sponsors/drizzy"
  },
  "categories": [
    "Programming Languages",
    "Other"
  ],
  "tags": [
    "C",
    "C++",
    "cpp",
    "Go",
    "Golang",
    "java",
    "javac",
    "run",
    "compiler",
    "build"
  ],
  "activationEvents": [
    "onStartupFinished",
    "onCommand:code-make-create.run",
    "onCommand:code-make-start.run"
  ],
  "main": "./out/src/extension.js",
  "scripts": {
    "vscode:prepublish": "npm run compile",
