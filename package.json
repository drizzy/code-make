{
  "name": "code-make",
  "displayName": "Code Make",
  "description": "Simplify C, C++, Go, and Java project creation and compilation with Make.",
  "version": "3.3.1",
  "publisher": "drizzy",
  "license": "MIT",
  "icon": "icon/makefile.png",
  "engines": {
    "vscode": "^1.75.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/drizzy/code-make.git"
  },
  "homepage": "https://github.com/drizzy/code-make/blob/master/README.md",
  "bugs": {
    "url": "https://github.com/drizzy/code-make/issues"
  },
  "author": {
    "name": "Willow P.",
    "email": "willow@drizzy.dev",
    "url": "https://drizzy.dev"
  },
  "sponsor": {
    "url": "https://github.com/sponsors/drizzy"
  },
  "categories": [
    "Programming Languages",
    "Other"
  ],
  "tags": [
    "C",
    "C++",
    "Go",
    "java",
    "make"
  ],
  "activationEvents": [
    "onStartupFinished"
  ],
  "contributes": {
    "commands": [
      {
        "command": "code-make-create.run",
        "title": "Code Make: Create Project"
      },
      {
        "command": "code-make-start.run",
        "title": "Code Make: Run Project"
      },
      {
        "command": "code-make-stop.run",
        "title": "Code Make: Stop Execution"
      }
    ],
    "keybindings": [
      {
        "command": "code-make-create.run",
        "key": "ctrl+alt+c"
      },
      {
        "command": "code-make-start.run",
        "key": "ctrl+alt+r"
      },
      {
        "command": "code-make-stop.run",
        "key": "ctrl+alt+s"
      }
    ]
  },
  "main": "./out/src/extension.js",
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/lodash": "^4.14.202",
    "@types/mocha": "^10.0.6",
    "@types/node": "18.x",
    "@types/vscode": "^1.75.0",
    "@typescript-eslint/eslint-plugin": "^6.13.1",
    "@typescript-eslint/parser": "^6.13.1",
    "@vscode/test-cli": "^0.0.4",
    "@vscode/test-electron": "^2.3.8",
    "eslint": "^8.54.0",
    "glob": "^11.0.1",
    "lodash": "^4.17.21",
    "ps-list": "^8.1.1",
    "typescript": "^5.3.2"
  }
}
